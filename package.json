    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.0",
    "jest": "^27.3.1",
    "pre-commit": "^1.2.2"
  },
  "precommit": "build",
  "homepage": "https://github.com/Pythe1337N/swedish-holidays#readme",
  "bugs": {
