  "scripts": {
    "build": "babel ./src --out-dir ./dist -s inline --delete-dir-on-start",
    "prepack": "npm run build",
    "test": "npm run build && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pythe1337N/swedish-holidays.git"
  },
  "keywords": [
